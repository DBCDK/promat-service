{
  "appName": "promat-service",
  "jdbc": {
    "jdbc/promat": {
      "url": "${PROMAT_DB_URL}"
    }
  },
  "hazelcast": {
    "clusterName": "${PROMAT_CLUSTER_NAME}"
  },
  "mail": {
    "mail/delivery": {
      "mailhost": "${MAIL_HOST}",
      "from": "${MAIL_FROM}",
      "user": "${MAIL_USER}"
    }
  }

}
